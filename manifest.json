{
    "name": "Look up At Multi Function Chinese Character Database",
    "description": "Look up At Multi Function Chinese Character Database",
    "version": "0.1",
    //  "permissions": ["contextMenus","browsingData","clipboardRead","clipboardWrite"],
    "permissions": ["contextMenus",
        "tabs",
        "http://*/*",
        "https://*/*"
